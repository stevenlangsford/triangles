  <body>
    <!-- predef html resources -->
    <div id="uberdiv" class='centered'>
      <h1>Thank you</h1>
    </div>
          <div>You're done!</div>
    <div>
      <p>Your completion code is: <span id='completioncodespan'></span></p>
    </div>
<script>
  var symbols = ["A","B","C","D","F","G","H","J","K","L","M","N","P","Q","R","S","T","V","W","X","Y","Z","1","2","3","4","5","6","7","8","9","1","2","3","4","5","6","7","8","9"];
  function getSymbol(){
  return symbols[Math.floor(Math.random()*symbols.length)]
  }
  function getCode(){
  return "UM"+getSymbol()+getSymbol()+"SL"+getSymbol()+getSymbol()+getSymbol()+"1";//some fixed vals so bad codes pop out. Zero defence against malice but sometimes folks make mistakes or write messages in the submission box.
  }
      document.getElementById("completioncodespan").innerHTML=(getCode());
</script>
  </body>
